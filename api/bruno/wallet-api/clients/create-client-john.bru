meta {
  name: Create Client - John Doe
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/clients
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "name": "John Doe",
    "email": "john@example.com"
  }
}

script:post-response {
  if (res.status === 200 || res.status === 201) {
    bru.setVar("johnClientId", res.body.ID);
  }
}
