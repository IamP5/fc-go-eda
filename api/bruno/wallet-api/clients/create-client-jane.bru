meta {
  name: Create Client - Jane Smith
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/clients
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "name": "Jane Smith",
    "email": "jane@example.com"
  }
}

script:post-response {
  if (res.status === 200 || res.status === 201) {
    bru.setVar("janeClientId", res.body.ID);
  }
}
